{
	"name": "Wrole",
	"version": "1.0.0",
	"description": "extension para ayudar a resolver wordle",
	"manifest_version": 3,
	"author": "lpdla",
	"action": {
		"default_popup": "index.html",
		"default_title": "Wordle helper"
	},
	"content_scripts": [{
		"matches": ["*://*.wordle.danielfrg.com/*", "https://wordleespanol.org/"],
		"js": ["content.js"]
	}],
	"permissions": ["activeTab"]
}